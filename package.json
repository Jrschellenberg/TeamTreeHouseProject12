{
  "name": "teamtreehouseproject12",
  "version": "1.0.0",
  "description": "Final Project for tech Degree TeamTreeHouse",
  "main": "index.js",
  "scripts": {
    "watchVue": "parcel watch src/client/*/** --out-dir src/public/js/dist/",
    "buildVue": "parcel build src/client/Vue.js --out-dir src/public/js/dist/",
    "buildSass": "parcel build src/assets/sass/styles.js --out-dir src/public/stylesheets/",
    "buildJSDependencies": "parcel build src/assets/js/dependencies.js --out-dir src/public/js/dependencies",
    "nodemon": "nodemon -e pug,js,css --exec npm run babel-node -- src/bin/www",
    "start": "cross-env NODE_ENV=development && npm run buildJSDependencies && concurrently --kill-others \"npm run watchSass\" \"npm run nodemon\" \"npm run watchVue\"",
    "babel-node": "babel-node --presets=es2015",
    "build": "babel src -d dist --presets=es2015 --copy-files public/*/** --ignore client/*",
    "test": "mocha --exit --timeout 20000 --compilers js:babel-register",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --exit --timeout 10000 --compilers js:babel-core/register",
    "lint": "jshint src/.",
    "deployProduction": "npm run buildJSDependencies && npm run buildVue && npm run buildSass && npm run build",
    "watchSass": "parcel watch src/assets/sass/styles.js --out-dir src/public/stylesheets/",
    "production": "npm run deployProduction && npm run runProduction",
    "runProduction": "cross-env NODE_ENV=production node dist/bin/www"
  },
  "author": "Justin Schellenberg",
  "license": "MIT",
  "dependencies": {
    "algorithmia": "^0.3.10",
    "async": "^2.6.1",
    "axios": "^0.18.0",
    "body-parser": "~1.18.2",
    "bootstrap": "^4.1.2",
    "bunyan": "^1.8.12",
    "config": "^1.30.0",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-rate-limit": "^2.11.0",
    "express-session": "^1.15.6",
    "jquery": "^3.2.1",
    "mongoose": "^5.2.3",
    "mongoose-seed": "^0.5.0",
    "morgan": "~1.9.0",
    "node-geocoder": "^3.22.0",
    "page": "^1.8.6",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "popper.js": "^1.14.1",
    "pug": "^2.0.3",
    "serve-favicon": "^2.5.0",
    "twilio": "^3.19.2",
    "vue": "^2.5.16",
    "vue2-google-maps": "^0.10.0"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.1.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-watch": "^2.0.7",
    "chai": "^4.1.2",
    "chai-dom": "^1.8.0",
    "chai-http": "^4.0.0",
    "chai-spies": "^1.0.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.2.0",
    "express-sanitizer": "^1.0.4",
    "jshint": "^2.9.5",
    "mocha": "^5.1.1",
    "mocha-jshint": "^2.3.1",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.2",
    "nyc": "^12.0.2",
    "parcel-bundler": "^1.9.7",
    "parcel-plugin-vue": "^1.5.0",
    "snyk": "^1.88.2"
  }
}
